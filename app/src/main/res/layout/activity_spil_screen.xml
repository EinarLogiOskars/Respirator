<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#292929"
    tools:context="com.dtucolab.myapplication.SpilScreen">

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="1280dp"
        android:layout_height="100dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/kald"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.997" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/krydsogbolle"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintHorizontal_bias="0.01"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="24dp"
        android:background="@drawable/quiz"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintLeft_toRightOf="@+id/imageButton11" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="24dp"
        android:background="@drawable/huskespil"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintLeft_toRightOf="@+id/imageButton12" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="24dp"
        android:background="@drawable/skak"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintLeft_toRightOf="@+id/imageButton13" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="160dp"
        android:layout_height="120dp"
        android:layout_marginBottom="9dp"
        android:layout_marginLeft="24dp"
        android:background="@drawable/vendespil"
        app:layout_constraintBottom_toTopOf="@+id/imageButton10"
        app:layout_constraintLeft_toRightOf="@+id/imageButton14"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/imageButton16"
        android:layout_width="100dp"
        android:layout_height="140dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/home"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
