<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.group5.respirator.Settings.Fragments.TouchFragment">


    <Button
        android:id="@+id/questionbutton2"
        android:layout_width="300dp"
        android:layout_height="180dp"
        android:layout_marginEnd="860dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/setting2"
        android:drawableTop="@drawable/ic_touch_app_black_120dp"
        android:drawableTint="@android:color/white"
        android:drawablePadding="2dip"
        android:paddingTop="20dip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/questionbutton3"
        android:layout_width="300dp"
        android:layout_height="180dp"
        android:layout_marginEnd="120dp"
        android:layout_marginStart="860dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/setting2"
        android:drawableTop="@drawable/ic_face_black_120dp"
        android:drawableTint="@android:color/white"
        android:drawablePadding="2dip"
        android:paddingTop="25dip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="175dp"
        android:layout_marginTop="150dp"
        android:text="Standard styring"
        android:textColor="@android:color/white"
        android:textSize="?attr/font_small"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="234dp"
        android:layout_marginTop="150dp"
        android:text="Hoved"
        android:textColor="@android:color/white"
        android:textSize="?attr/font_small"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="55dp"
        android:layout_marginEnd="337dp"
        android:layout_marginStart="338dp"
        android:layout_marginTop="46dp"
        android:text="1. Hvordan vil patientien bruge appen?"
        android:textColor="@android:color/white"
        android:textSize="?attr/font_xlarge"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
