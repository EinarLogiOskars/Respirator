        <activity android:name="com.example.bijannegari.galgespilprojekt.ChooseWordManually" />
        <activity android:name="com.example.bijannegari.galgespilprojekt.ListViewMedArrayAdapter" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />
        <meta-data
